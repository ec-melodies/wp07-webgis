<?xml version="1.0" encoding="ISO-8859-1"?>
<StyledLayerDescriptor version="1.0.0"
                       xmlns="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc"
                       xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                       xsi:schemaLocation="http://www.opengis.net/sld      http://schemas.opengis.net/sld/1.0.0/StyledLayerDescriptor.xsd">
    <NamedLayer>
        <Name>LULC</Name>
        <UserStyle>
            <FeatureTypeStyle>
                <Rule>
                    <RasterSymbolizer>
                        <ColorMap type="values">
                            <ColorMapEntry color="#000000" quantity="0" label="No data" opacity="0"/>
                            <ColorMapEntry color="#ff0000" quantity="1" label="Urban fabric" opacity="1"/>
                            <ColorMapEntry color="#ffbebe" quantity="2" label="Rainfed croplands" opacity="1"/>
                            <ColorMapEntry color="#ffaa00" quantity="3" label="Irrigated croplands" opacity="1"/>
                            <ColorMapEntry color="#38a800" quantity="4" label="Forests" opacity="1"/>
                            <ColorMapEntry color="#a8a800" quantity="5" label="Shrubland" opacity="1"/>
                            <ColorMapEntry color="#a3ff73" quantity="6" label="Natural herbaceous vegetation" opacity="1"/>
                            <ColorMapEntry color="#e9ffbe" quantity="7" label="Sparse vegetation" opacity="1"/>
                            <ColorMapEntry color="#686868" quantity="8" label="Burnt areas" opacity="1"/>
                            <ColorMapEntry color="#ffebbe" quantity="9" label="Bare soil" opacity="1"/>
                            <ColorMapEntry color="#beffe8" quantity="10" label="Wetlands" opacity="1"/>
                            <ColorMapEntry color="#0070ff" quantity="11" label="Water" opacity="1"/>
                        </ColorMap>
                    </RasterSymbolizer>
                </Rule>
            </FeatureTypeStyle>
        </UserStyle>
    </NamedLayer>
</StyledLayerDescriptor>
